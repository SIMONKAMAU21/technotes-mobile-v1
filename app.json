{
  "expo": {
    "name": "schoolmanagmen-v1",
    "slug": "schoolmanagmen-v1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/school2.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jahnnet.schoolmanagmenv1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/school2.png",
        "backgroundColor": "#000000"
      },
      "package": "com.jahnnet.schoolmanagmenv1"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/school2.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1f3db18b-53da-43cc-8878-50245b3af4e9"
      }
    }
  }
}
